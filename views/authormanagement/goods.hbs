<br>
{{#each goods}}
    <div style="width: 470px;height:500px;display:block;float: left;text-align: center">
        <img style="width: 430px;height: 300px" src="{{this.imgurl}}"/>
        <font>{{this.name}}</font><font>{{this.price}}</font><br>
        <a href="/shopping/{{this._id}}">add to shopping</a>
    </div>
{{/each}}